                <rdf:rest>
                  <rdf:Description>
                    <rdf:first rdf:resource="http://purl.oclc.org/fast/ontology/gadget#Operator"/>
                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                  </rdf:Description>
                </rdf:rest>
              </rdf:Description>
            </rdf:rest>
          </rdf:Description>
        </rdf:rest>
      </rdf:Description>
    </owl:unionOf>
  </owl:Class>
  <owl:Class rdf:about="http://purl.oclc.org/fast/ontology/gadget#WithDefinition">
    <misc:subject rdf:resource="http://purl.oclc.org/fast/ontology/gadget#TopicDefiningBuildingBlocks"/>
    <rdfs:comment xml:lang="en">Any kind of building block that can be defined declaratively in the GVS.</rdfs:comment>
    <rdfs:label xml:lang="en">With Definition</rdfs:label>
    <owl:unionOf>
      <rdf:Description>
        <rdf:first rdf:resource="http://purl.oclc.org/fast/ontology/gadget#Screen"/>
