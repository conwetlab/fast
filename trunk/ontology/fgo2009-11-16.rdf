    <misc:subject rdf:resource="http://purl.oclc.org/fast/ontology/gadget#TopicConditions"/>
    <rdfs:comment xml:lang="en">Those kinds of building blocks which can have post-conditions.</rdfs:comment>
    <rdfs:label xml:lang="en">With Post-condition</rdfs:label>
    <owl:unionOf>
      <rdf:Description>
        <rdf:first rdf:resource="http://purl.oclc.org/fast/ontology/gadget#ScreenFlow"/>
