<?xml version="1.0" encoding="UTF-8" ?>
<Module>
<ModulePrefs title="{{ gadgetTitle }}"
        title_url="{{ gadgetUri }}/google.xml"
        author="{{ gadgetVendor }}"
        author_email="{{ gadgetMail }}"
        description="{{ gadgetDescription }}"
        scrolling="true" height="{{ gadgetHeight }}" width="{{ gadgetWidth }}">
    <Require feature="dynamic-height"/>
    <Require feature="opensocial-0.7"/>
</ModulePrefs>
<Content type="html">
<![CDATA[
{% block content %}{% endblock %}
]]>
</Content>
</Module>
