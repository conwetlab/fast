{
    "name":"Compound Amazon Price Comparative Screen",
    "creationDate":"2009-04-20T17:00:00+0100",
    "creator":"http://www.fabio.es/",
    "description":{
        "en-gb":"This screen shows a price comparative for a given product."
    },
    "domainContext":{
        "tags":[
            "compound",
            "amazon",
            "list",
            "suggestion",
            "price comparative"
        ],
        "user":"admin"
    },
    "homepage":"http://www.deri.ie/amazonPrice",
    "icon":"http://demo.fast.morfeo-project.org/gvsdata/images/catalogue/amazonPrice.png",
    "label":{
        "en-gb":"Price Comparative Compound"
    },
    "rights":"http://creativecommons.org/",
    "screenshot":"http://demo.fast.morfeo-project.org/gvsdata/images/screenshots/amazonPriceComparative.png",
    "version":"1.0",
    "definition":{
        "buildingblocks":[
            {
                "id":"PriceComparativeForm1",
                "uri":"http://purl.oclc.org/fast/ontology/gadget#FormElement1259253572039"
            },
            {
                "id":"PriceComparativeService1",
                "uri":"http://purl.oclc.org/fast/ontology/gadget#BackendService1259254187946"
            },
            {
                "id":"AmazonAddOfferToCartService1",
                "uri":"http://purl.oclc.org/fast/ontology/gadget#BackendService1259254005724"
            }
        ],
        "pipes":[
            {
                "from":{
                    "buildingblock":"PriceComparativeService1",
                    "condition":"list"
                },
                "to":{
                    "buildingblock":"PriceComparativeForm1",
                    "condition":"list",
                    "action":"showTable"
                }
            },
            {
                "from":{
                    "buildingblock":"PriceComparativeService1",
                    "condition":"message"
                },
                "to":{
                    "buildingblock":"PriceComparativeForm1",
                    "condition":"message",
                    "action":"showMessage"
                }
            },
            {
                "from":{
                    "buildingblock":"AmazonAddOfferToCartService1",
                    "condition":"message"
                },
                "to":{
                    "buildingblock":"PriceComparativeForm1",
                    "condition":"message",
                    "action":"showMessage"
                }
            },
            {
                "from":{
                    "buildingblock":"PriceComparativeForm1",
                    "condition":"offer"
                },
                "to":{
                    "buildingblock":"AmazonAddOfferToCartService1",
                    "condition":"offer",
                    "action":"addToCart"
                }
            },
            {
                "from":{
                    "buildingblock":"AmazonAddOfferToCartService1",
                    "condition":"cart"
                },
                "to":{
                    "buildingblock":"",
                    "action":"",
                    "condition":"cart"
                }
            }
        ],
        "triggers":[
            {
                "from":{
                    "buildingblock":"",
                    "name":"_onload"
                },
                "to":{
                    "buildingblock":"PriceComparativeForm1",
                    "action":"init"
                }
            },
            {
                "from":{
                    "buildingblock":"",
                    "name":"_onload"
                },
                "to":{
                    "buildingblock":"PriceComparativeService1",
                    "action":"fetch"
                }
            }
        ]
    },
    "preconditions":[
        [
            {
                "label":{
                    "en-gb":"An item"
                },
                "pattern":"?Item http://www.w3.org/1999/02/22-rdf-syntax-ns#type http://aws.amazon.com/AWSECommerceService#Item",
                "positive":"true"
            }
        ]
    ],
    "postconditions":[
        [
            {
                "id":"cart",
                "label":{
                    "en-gb":"An shopping cart"
                },
                "pattern":"?Cart http://www.w3.org/1999/02/22-rdf-syntax-ns#type http://aws.amazon.com/AWSECommerceService#ShoppingCart",
                "positive":"true"
            }
        ]
    ]
}